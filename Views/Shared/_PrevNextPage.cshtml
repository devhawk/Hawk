@model Tuple<string, string>

<div class="prev-next-page">
    @if (!string.IsNullOrWhiteSpace(Model.Item1))
    {
        <a class="btn btn-primary" href="@Model.Item1">← Newer Posts</a>
    }

    @if (!string.IsNullOrWhiteSpace(Model.Item2))
    {
        <a class="btn btn-primary" href="@Model.Item2">Older Posts →</a>
    }
</div>
